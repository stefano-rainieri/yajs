{
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "mocha": true,
    "es6": true,
    "es2017": true,
    "es2020": true
  },
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module",
    "ecmaFeatures": {}
  },
  "rules": {
    "arrow-parens": 2,
    "arrow-spacing": 2,
    "class-methods-use-this": 2,
    "constructor-super": 2,
    "default-param-last": 2,
    "eol-last": 2,
    "eqeqeq": 2,
    "for-direction": 2,
    "generator-star-spacing": 2,
    "getter-return": 2,
    "lines-between-class-members": 2,
    "no-case-declarations": 2,
    "no-class-assign": 2,
    "no-compare-neg-zero": 2,
    "no-cond-assign": 2,
    "no-const-assign": 2,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-delete-var": 2,
    "no-dupe-args": 2,
    "no-dupe-class-members": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-empty-pattern": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-fallthrough": 2,
    "no-func-assign": 2,
    "no-global-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-misleading-character-class": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-new-symbol": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-prototype-builtins": 2,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-self-assign": 2,
    "no-shadow-restricted-names": 2,
    "no-sparse-arrays": 2,
    "no-tabs": 2,
    "no-this-before-super": 2,
    "no-undef": 2,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "no-unsafe-finally": 2,
    "no-unsafe-negation": 2,
    "no-unused-labels": 2,
    "no-unused-vars": 2,
    "no-useless-catch": 2,
    "no-useless-escape": 2,
    "no-var": 1,
    "no-with": 2,
    "prefer-arrow-callback": 2,
    "prefer-const": 2,
    "prefer-template": 2,
    "quotes": [
      2,
      "single"
    ],
    "require-yield": 2,
    "space-before-blocks": 2,
    "use-isnan": 2,
    "valid-jsdoc": 2,
    "valid-typeof": 2,
    "yield-star-spacing": 2,
    "yoda": [
      2,
      "always"
    ]
  }
}